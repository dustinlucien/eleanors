function updateImageOrientation(){
  var scrollAmount = 320;
  var imageMax = jQuery(".swipe-controls li").size() - 1;
  if(orientationType == "horizontal"){
    scrollAmount = 480;
  }
  var currentMargin = -(current_image * scrollAmount);
  jQuery(".swipe").css('margin-left', currentMargin+'px')
}

function swipeImages(){
  var scrollAmount = 320;
  var imageMax = jQuery(".swipe-controls li").size() - 1;
  if(orientationType == "horizontal"){ scrollAmount = 480; }
  
  var currentMargin = -(current_image * scrollAmount);
  
  jQuery(".swipe").css('margin-left', currentMargin+'px')
  
  var tmp = parseInt(jQuery(".swipe").css('margin-left'));
  
  if(((left_x - right_x) > 40) && (current_image < imageMax)){
     tmp -= scrollAmount;
     current_image++;
  }
  
  if(((right_x - left_x) > 40) && (current_image > 0)){
     tmp += scrollAmount;
     current_image--;
  }

  jQuery(".swipe").animate({'margin-left': tmp+'px'});
  jQuery(".swipe-controls li").each(function(){ jQuery(this).removeClass('active'); });
  jQuery(".swipe-controls li").eq(current_image).addClass('active');
}
 
function updateOrientation(){
  if(window.orientation == 90 || window.orientation == -90){
    orientationType = "horizontal";
  } else {
    orientationType = "vertical";
  }
  jQuery('body').removeClass('vertical horizontal').addClass(orientationType);
  updateImageOrientation();
}
  
