jQuery(window).load(function(){
  
  jQuery(".swipe").css('height', jQuery(".swipe > div").eq(0).css('height'));
  
  if(jQuery(".swipe-controls ul li").size() > 1){
    jQuery(".swipe-controls ul").fadeIn('slow');
  } else {
    jQuery(".swipe-controls").hide();
  }
  
  jQuery(".swipe > div").bind('touchstart', function(e){
    left_x = e.originalEvent.touches[0].pageX;
    right_x = null;
  });
  
  jQuery(".swipe > div").bind('touchmove', function(e){
    //e.preventDefault(); // comment out this line to allow vertical scrolling on swipe div
    right_x = e.originalEvent.touches[0].pageX;
  });
  
  jQuery(".swipe > div").bind('touchend', function(e){
    if(right_x == null){right_x = left_x;}
    if( ((left_x - right_x) > 40) || ((right_x - left_x) > 40) ){ // if there was a swipe
      swipeImages();
    } else { // if there was no swipe
      var url = jQuery(this).children('img').attr('data-url');
      if (url != undefined && url.length > 0) {window.location.href = url;}
    }
  });
  
}); // end window load

